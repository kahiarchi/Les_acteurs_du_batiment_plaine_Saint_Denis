@prefix local: <http://example.org/local/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# CLASSE
local:Participation a owl:Class ;
    rdfs:label "Participation citoyenne (locale)"@fr .

# PROPRIÉTÉ NUMÉRIQUE
local:participantsNumberLocal a owl:DatatypeProperty ;
    rdfs:label "nombre de participants (local)"@fr ;
    rdfs:domain local:Participation ;
    rdfs:range xsd:integer ;
    rdfs:comment "Nombre total de participants pour visualisation"@fr .

local:participationType a owl:DatatypeProperty ;
    rdfs:label "type de participation"@fr ;
    rdfs:domain local:Participation ;
    rdfs:range xsd:string .

local:relatedToProject a owl:DatatypeProperty ;
    rdfs:label "lié au projet"@fr ;
    rdfs:domain local:Participation ;
    rdfs:range xsd:string .

# ITEMS

# Atelier de quartier
local:AtelierQuartier a local:Participation ;
    rdfs:label "Atelier de quartier"@fr ;
    local:participantsNumberLocal "50"^^xsd:integer ;
    local:participationType "atelier de quartier" ;
    local:relatedToProject "EcoQuartier Plaine" .

# Participation numérique
local:ParticipationNumerique a local:Participation ;
    rdfs:label "Participation numérique"@fr ;
    local:participantsNumberLocal "120"^^xsd:integer ;
    local:participationType "concertation numérique" ;
    local:relatedToProject "EcoQuartier Plaine" .
